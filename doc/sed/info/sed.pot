#. type: item
#: sed.texi:2551
#, no-wrap
msgid "[:blank:]"
msgstr ""

#. type: opindex
#: sed.texi:2552
#, no-wrap
msgid "blank @r{character class}"
msgstr ""

#. type: cindex
#: sed.texi:2553
#, no-wrap
msgid "blank characters"
msgstr ""

#. type: table
#: sed.texi:2556
msgid "Blank characters: space and tab."
msgstr ""

#. type: item
#: sed.texi:2557
#, no-wrap
msgid "[:cntrl:]"
msgstr ""

#. type: opindex
#: sed.texi:2558
#, no-wrap
msgid "cntrl @r{character class}"
msgstr ""

#. type: cindex
#: sed.texi:2559
#, no-wrap
msgid "control characters"
msgstr ""

#. type: table
