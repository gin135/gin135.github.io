#: gawk.1:3948
#, no-wrap
msgid "EXAMPLES"
msgstr "例"

#. type: Plain text
#: gawk.1:3951
#, no-wrap
msgid "Print and sort the login names of all users:\n"
msgstr ""

#. type: Plain text
#: gawk.1:3955
#, no-wrap
msgid ""
"B<\tBEGIN\t{ FS = \":\" }\n"
"\t\t{ print $1 | \"sort\" }>\n"
msgstr ""
"B<\tBEGIN\t{ FS = \":\" }\n"
"\t\t{ print $1 | \"sort\" }>\n"

#. type: Plain text
#: gawk.1:3958
#, no-wrap
msgid "Count lines in a file:\n"
msgstr ""

#. type: Plain text
#: gawk.1:3962
#, no-wrap
msgid ""
"B<\t\t{ nlines++ }\n"
"\tEND\t{ print nlines }>\n"
msgstr ""
"B<\t\t{ nlines++ }\n"
"\tEND\t{ print nlines }>\n"

#. type: Plain text
#: gawk.1:3965
#, no-wrap
msgid "Precede each line by its number in the file:\n"
msgstr ""

#. type: Plain text
#: gawk.1:3968
#, no-wrap
msgid "B<\t{ print FNR, $0 }>\n"
msgstr "B<\t{ print FNR, $0 }>\n"

#. type: Plain text
#: gawk.1:3971
#, no-wrap
msgid "Concatenate and line number (a variation on a theme):\n"
msgstr ""

#. type: Plain text
#: gawk.1:3974
#, no-wrap
msgid "B<\t{ print NR, $0 }>\n"
msgstr "B<\t{ print NR, $0 }>\n"

#. type: Plain text
#: gawk.1:3977
#, no-wrap
msgid "Run an external command for particular lines of data:\n"
msgstr ""

#. type: Plain text
#: gawk.1:3981
#, no-wrap
msgid ""
"B<\ttail -f access_log |\n"
"\tawk '/myhome.html/ { system(\"nmap \" $1 \"E<gt>E<gt> "
"logdir/myhome.html\") }'>\n"
msgstr ""
"B<\ttail -f access_log |\n"
"\tawk '/myhome.html/ { system(\"nmap \" $1 \"E<gt>E<gt> "
"logdir/myhome.html\") }'>\n"

